
<div class="flickr">
	<% photos.each do |row| %>
		<div class="row">	
			<% row.each do |p| %>

				<%if p.present?%>
					<div class="col-md-3">
						<div class="thumbnail">
							<%=link_to(image_tag(FlickRaw.url_q(p), :class => 'flickr_photo', :title=> p.title, :border => 0, :size => '200x200'), FlickRaw.url_photopage(p)) %>
						</div>
					</div>
				<%end%>
			<%end%>
		</div>
	<%end%>
</div>